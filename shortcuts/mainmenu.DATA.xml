	<shortcut>
        <label>Music</label>
        <label2>Common Shortcut</label2>
        <defaultID>music</defaultID>
        <icon>special://skin/extras/icons/songs.png</icon>
        <action>ActivateWindow(MusicLibrary)</action>
		<visible>IntegerGreaterThan(Window(Home).Property(Music.SongsCount),0)</visible>
		<visible>!System.HasAddon(plugin.video.emby) | !IsEmpty(Window(Home).Property(emby.nodes.0.title))</visible>
    </shortcut>
